<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="35dp"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <AbsoluteLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginTop="15dp"
            android:layout_x="19dp"
            android:layout_y="12dp"
            android:text="Nome:"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/nomeText"
            android:layout_width="287dp"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_x="66dp"
            android:layout_y="12dp"
            android:text="Nome"
            android:textStyle="bold" />


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_x="18dp"
            android:layout_y="47dp"
            android:text="Emdereço:"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/endeText"
            android:layout_width="260dp"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_x="93dp"
            android:layout_y="47dp"
            android:text="Endereço" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignTop="@+id/nomeText"
            android:layout_marginStart="121dp"
            android:layout_x="18dp"
            android:layout_y="85dp"
            android:text="Telefone:"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/tellText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="142dp"
            android:layout_x="93dp"
            android:layout_y="87dp"
            android:text="Telefone"
            />
    </AbsoluteLayout>


</androidx.constraintlayout.widget.ConstraintLayout>